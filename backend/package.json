{
  "name": "feest-backend",
  "version": "1.0.0",
  "description": "Backend API for Feest - Food sharing and swap application",
  "main": "dist/server.js",  
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "tsc && tsc-alias",
    "test": "jest",
    "seed": "tsx -r tsconfig-paths/register src/database/seed.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "tsx -r tsconfig-paths/register src/database/seed.ts",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "rate-limiter-flexible": "^5.0.3",
    "socket.io": "^4.7.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "food-sharing",
    "express",
    "postgresql",
    "typescript",
    "api"
  ],
  "author": "Feest Team",
  "license": "MIT"
}
